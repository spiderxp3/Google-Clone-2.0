                    d="M34.93 40.94v-9.41h31.71c.31 1.64.47 3.58.47 5.68 0 7.06-1.93 15.79-8.15 22.01-6.05 6.3-13.78 9.66-24.02 9.66C15.96 68.88 0 53.42 0 34.44 0 15.46 15.96 0 34.94 0c10.5 0 17.98 4.12 23.6 9.49l-6.64 6.64c-4.03-3.78-9.49-6.72-16.97-6.72-13.86 0-24.7 11.17-24.7 25.03 0 13.86 10.84 25.03 24.7 25.03 8.99 0 14.11-3.61 17.39-6.89 2.66-2.66 4.41-6.46 5.1-11.65z"
                    fill="#4285F4" /></svg>
        </div>
        <div>
            <form action="https://google.com/search" method="GET" autocomplete="off" class="main__form">
                <div class="main__form-top">

                    <!-- SEARCH -->
                    <span class="main__search-icon">
                        <svg focusable="false" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="20">
                            <path fill="#9aa0a6"
                                d="M15.5 14h-.79l-.28-.27A6.471 6.471 0 0 0 16 9.5 6.5 6.5 0 1 0 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z">
                            </path>
                        </svg>
                    </span>

                    <!-- INPUT -->
                    <div class="main__input-container">
                        <input class="main__input" type="search" name="q" title="Search">
                    </div>

                    <!-- AUDIO -->
                    <span class="main__audio-icon">
                        <svg class="main__audio-svg" focusable="false" viewBox="0 0 24 24"
                            xmlns="http://www.w3.org/2000/svg" width="20">
                            <path fill="#4285f4"
                                d="m12 15c1.66 0 3-1.31 3-2.97v-7.02c0-1.66-1.34-3.01-3-3.01s-3 1.34-3 3.01v7.02c0 1.66 1.34 2.97 3 2.97z">
                            </path>
                            <path fill="#34a853" d="m11 18.08h2v3.92h-2z"></path>
                            <path fill="#fbbc05"
                                d="m7.05 16.87c-1.27-1.33-2.05-2.83-2.05-4.87h2c0 1.45 0.56 2.42 1.47 3.38v0.32l-1.15 1.18z">
                            </path>
                            <path fill="#ea4335"
                                d="m12 16.93a4.97 5.25 0 0 1 -3.54 -1.55l-1.41 1.49c1.26 1.34 3.02 2.13 4.95 2.13 3.87 0 6.99-2.92 6.99-7h-1.99c0 2.92-2.24 4.93-5 4.93z">
                            </path>
                        </svg>

                        <div class="main__audio-tooltip">
                            <p>Search by voice</p>
                        </div>
                    </span>
                </div>

                <!-- BUTTONS -->
                <div class="main__buttons">
                    <button class="main__button main__search-button" type="submit">Google Search</button>
                    <button class="main__button main__lucky-button" name="btnI" type="submit">I'm Feeling Lucky</button>
                </div>
            </form>
        </div>
        <div class="main__lang">
            <span>
                Google offered in:
                <a class="main__lang-link" href="#">English</a>
                <a class="main__lang-link" href="#">India</a>
            </span>
        </div>
    </main>

    <!-- FOOTER -->
    <footer class="footer">
        <div class="footer__location">
            <span class="footer__location-exact">India</span>
        </div>
        <span class="footer__line-break"></span>
        <div class="footer__nav">
            <ul class="footer__list">
